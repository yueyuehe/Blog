.titleSpanA{ transform:90deg; -webkit-transform:90deg; -moz-transform:90deg; -o-transform:90deg;}
.gnTitle{ width:250px; height:56px; line-height:56px; padding-left:36px; background:#f0f3f7; color:#828890; font-size:18px; border-right:1px solid #e9e9e9;border-bottom:1px solid #e9e9e9;}

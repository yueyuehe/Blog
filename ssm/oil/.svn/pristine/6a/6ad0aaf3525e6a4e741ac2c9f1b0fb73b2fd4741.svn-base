.user i{display:inline-block;margin-right:5px;font-style:normal;line-height:30px; font-size:14px;color:#b8ceda;}
.userNameIc{background:url(../images/userIcon.png) no-repeat 25px center;}
.exitIc{background:url(../images/exitIcon.png) no-repeat 25px center;}



/*left.html*/
.lefttop{background:url(../images/leftLogo.gif) repeat-x;height:72px; width:270px; overflow:hidden;}

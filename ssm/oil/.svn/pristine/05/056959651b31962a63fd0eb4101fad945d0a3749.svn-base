 @-webkit-keyframes twinkling{    /*透明度由0到1*/
    0%{
       opacity:0.6; /*透明度为0*/
     }
    25%{
       opacity:0.8; /*透明度为0*/
     }
    50%{
       opacity:1; /*透明度为0*/
     }
     75%{
       opacity:0.8; /*透明度为0*/
     }
    100%{
       opacity:0.6; /*透明度为1*/
    }
  }
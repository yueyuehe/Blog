<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hm.oil.dao.BaseResourceDao">
	<insert id="insertBaseResource" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO base_resource(pl_id,pl_spec_id,pl_section_id,mpa,throughout,length,chamber_well,fangfu_station,
		admin,disable) 
		VALUES(#{pl_id},#{pl_spec_id},#{pl_section_id},#{mpa},#{throughout},#{length},#{chamber_well},#{fangfu_station},
		#{admin},#{disable})
	</insert>
</mapper>